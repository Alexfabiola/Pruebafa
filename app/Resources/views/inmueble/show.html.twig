{% extends 'base.html.twig' %}

{% block body %}
    <h1>Inmueble</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ inmueble.id }}</td>
            </tr>
            <tr>
                <th>Ubicacion</th>
                <td>{{ inmueble.ubicacion }}</td>
            </tr>
            <tr>
                <th>Tipo</th>
                <td>{{ inmueble.tipo }}</td>
            </tr>
            <tr>
                <th>Registro</th>
                <td>{{ inmueble.registro }}</td>
            </tr>
            <tr>
                <th>Impuesto</th>
                <td>{{ inmueble.impuesto }}</td>
            </tr>
            </tr>
                <th>Propietrario</th>
                <td>{{ inmueble.persona }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('inmueble_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('inmueble_edit', { 'id': inmueble.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
